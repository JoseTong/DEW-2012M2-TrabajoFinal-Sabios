<h1>Lista de Emprendedores</h1>

<table bgcolor= "#FFFFFF" border="2" cellpadding="1" cellspacing="1" >
  <tr>
    <th bgcolor= "#c0c0c0"><font color="black">Codigo</font></th>
    <th bgcolor= "#c0c0c0"><font color="black">Nombres</font></th>
    <th bgcolor= "#c0c0c0"><font color="black">Apellido Paterno</font></th>
    <th bgcolor= "#c0c0c0"><font color="black">Apellido Materno</font></th>
    <th bgcolor= "#c0c0c0"><font color="black">Sexo</font></th>
    <th bgcolor= "#c0c0c0"><font color="black">Tipo DI</font></th>
    <th bgcolor= "#c0c0c0"><font color="black">Numero di</font></th>
    <th bgcolor= "#c0c0c0"><font color="black">Email</font></th>
    <th bgcolor= "#c0c0c0"><font color="black">Celular</font></th>
    <th bgcolor= "#c0c0c0"><font color="black">Direccion</font></th>
    <th bgcolor= "#c0c0c0"><font color="black">FecNac</font></th>
    <th bgcolor= "#c0c0c0"><font color="black">Contraseña</font></th>
    <th bgcolor= "#c0c0c0"><font color="black">Apelativo</font></th>
    <th bgcolor= "#c0c0c0"><font color="black">Autobiografía</font></th>
    <th bgcolor= "#c0c0c0"><font color="black">web</font></th>
    <th bgcolor= "#c0c0c0"><font color="black">Cta. Bancaria</font></th>

  </tr>

<% @emprendedors.each do |emprendedor| %>
  <tr>
        <td><font color="black"><%= emprendedor.codsabio %></font></td>
        <td><font color="black"><%= emprendedor.nombres %></font></td>
        <td><font color="black"><%= emprendedor.apepaterno %></font></td>
        <td><font color="black"><%= emprendedor.apematerno %></font></td>
        <td><font color="black"><%= emprendedor.sexo %></font></td>
        <td><font color="black"><%= emprendedor.tipodi %></font></td>
        <td><font color="black"><%= emprendedor.numerodi %></font></td>
        <td><font color="black"><%= emprendedor.email %></font></td>
        <td><font color="black"><%= emprendedor.celular %></font></td>
        <td><font color="black"><%= emprendedor.direccion %></font></td>
        <td><font color="black"><%= emprendedor.fecnac %></font></td>
        <td><font color="black"><%= emprendedor.password %></font></td>
        <td><font color="black"><%= emprendedor.apelativo %></font></td>
        <td><font color="black"><%= emprendedor.autobigrafia %></font></td>
        <td><font color="black"><%= emprendedor.web %></font></td>
        <td><font color="black"><%= emprendedor.ctabancaria %></font></td>


    <td><%= link_to 'Visualizar', emprendedor %></td>
    <td><%= link_to 'Editar', edit_emprendedor_path(emprendedor) %></td>
    <td><%= link_to 'Eliminar', emprendedor, method: :delete, data: { confirm: 'Estas tu seguro de eliminar al emprendedor?' } %></td>
  </tr>
<% end %>
</table>

