<h1>Preguntas</h1>

<table>
  <tr>
	<th>Plan de Negocio</th>
    <th>Pregunta</th>
    <th>Respuesta</th>    
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @faqs.each do |faq| %>
  <tr>
	<td><%= faq.plannegocio.tituloplan %></td>
    <td><%= faq.pregunta %></td>
    <td><%= faq.respuesta %></td>    
    <td><%= link_to 'Mostrar', faq %></td>
    <td><%= link_to 'Editar', edit_faq_path(faq) %></td>
    <td><%= link_to 'Eliminar', faq, method: :delete, data: { confirm: 'Esta seguro de eliminar?' } %></td>
  </tr>
<% end %>
</table>

